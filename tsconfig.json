{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["@types/*"]
    },
    "target": "ES5",
    "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["dom", "es2015"],
    "sourceMap": true,
    "strict": true,
    "jsx": "react",
    "resolveJsonModule": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
